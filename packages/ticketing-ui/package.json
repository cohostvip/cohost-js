{
  "name": "@cohostvip/ticketing-ui",
  "version": "0.0.1",
  "description": "React UI components for Cohost ticketing",
  "license": "ISC",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./preset": {
      "import": "./dist/tailwind.preset.js",
      "types": "./dist/tailwind.preset.d.ts"
    },
    "./styles": "./dist/styles/variables.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup && cp src/styles/variables.css dist/styles/variables.css",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "@cohostvip/cohost-node": "*",
    "@cohostvip/cohost-react": "*",
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "tailwindcss": "^3.4.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.2",
    "@cohostvip/cohost-node": "^0.3.7",
    "@storybook/addon-essentials": "^8.5.0",
    "@storybook/addon-interactions": "^8.5.0",
    "@storybook/addon-links": "^8.5.0",
    "@storybook/blocks": "^8.5.0",
    "@storybook/react": "^8.5.0",
    "@storybook/react-vite": "^8.5.0",
    "@storybook/test": "^8.5.0",
    "@types/react": "^19.2.0",
    "@types/react-dom": "^19.2.0",
    "autoprefixer": "^10.4.0",
    "clsx": "^2.1.1",
    "postcss": "^8.4.0",
    "storybook": "^8.5.0",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.0",
    "typescript": "^5.9.0",
    "vite": "^6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cohostvip/cohost-js.git",
    "directory": "packages/ticketing-ui"
  },
  "author": "Cohost <support@cohost.vip> (https://cohost.vip)",
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "cohost",
    "ticketing",
    "react",
    "ui",
    "components",
    "tailwindcss"
  ]
}
